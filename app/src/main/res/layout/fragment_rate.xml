        app:layout_constraintBaseline_toBaselineOf="@+id/restacking_labTV"
        app:layout_constraintStart_toEndOf="@+id/restacking_labTV" />

    <TextView
        android:id="@+id/weightment_labTV"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
