        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="Shed To Wagon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/truck_to_platform_labTV" />

    <TextView
        android:id="@+id/shed_to_wagonTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/shed_to_wagon_labTV"
        app:layout_constraintStart_toEndOf="@+id/shed_to_wagon_labTV" />

    <TextView
        android:id="@+id/wagon_to_truck_labTV"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Wagon To Truck"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/shed_to_wagon_labTV" />

    <TextView
        android:id="@+id/wagon_to_truckTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/wagon_to_truck_labTV"
        app:layout_constraintStart_toEndOf="@+id/wagon_to_truck_labTV" />

    <TextView
        android:id="@+id/refilling_labTV"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Refilling"
        app:layout_constraintStart_toStartOf="parent"
