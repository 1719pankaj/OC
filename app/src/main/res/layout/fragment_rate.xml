    <TextView
        android:id="@+id/truck_to_truck_labTV"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Truck To Truck"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weightment_labTV" />

    <TextView
        android:id="@+id/truck_to_truckTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/truck_to_truck_labTV"
        app:layout_constraintStart_toEndOf="@+id/truck_to_truck_labTV" />
